<% if(success && success.length) { %> 
    <div class="flash" role="alert">
        <i class="fas fa-check" title="Dismiss"></i><%= success %> 
    </div>
<% } if(error && error.length)  { %> 
    <div class="flash" role="error">
        <i class="fas fa-times" title="Dismiss"></i><%= error %> 
    </div>
<% } %> 